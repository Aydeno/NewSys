<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<rule>
		<from>/wx/([A-Za-z0-9_\-]+).html</from>
		<to type="forward">/wx/$1.action</to>
	</rule>
	<rule>
		<from>/api/([A-Za-z0-9_\-]+)/([A-Za-z0-9_\-]+).html</from>
		<to type="forward">/api/$1/$2.action</to>
	</rule>
	<rule>
		<from>/api/([A-Za-z0-9_\-]+).html</from>
		<to type="forward">/api/$1.action</to>
	</rule>
	
</urlrewrite>